{
   "background": {
      "scripts": [ "ticker.js" ]
   },
   "browser_action": {
      "default_icon": "icon19.png"
   },
   "description": "KOMODO price ticker (price taken from 'coinmarketcap.com'). Extension shows KOMODO valued in Bitcoin and USD.",
   "icons": {
      "200": "icon200.png",
      "19": "icon19.png"
   },
   "manifest_version": 2,
   "name": "KOMODO price ticker",
   "short_name": "KOMODO ticker",
   "version": "0.0.1",
    "permissions": [
    "https://api.coinmarketcap.com/"
  ]
}
